package constructorarea;

public class Hello {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name;
		   //Constructor
		   Hello(){
		      this.name = "BeginnersBook.com";
		   }
		   public static void main(String[] args) {
		      Hello obj = new Hello();
		      System.out.println(obj.name);
		   }
	}

}
