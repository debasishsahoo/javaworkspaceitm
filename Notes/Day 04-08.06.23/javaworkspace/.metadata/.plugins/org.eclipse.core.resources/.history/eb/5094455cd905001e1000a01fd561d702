package problams;

public class Date {
	private int day;
	private int month;
	private int year;
	
	public Date(int d,int m,int y) {
		this.day=d;
		this.month=m;
		this.year=y;
	}
	
	public int getYear() {
		return this.year;
	};
	
	
	

	public static void main(String[] args) {
		Date obj=new date();

	}

}
